{{#section 'style'}}
    .cover {
      display: flex;
      flex-direction: column;
      height: 100%;
    }
    .content {
      flex: 1 0 auto;
    }
    #contact {
      flex-shrink: 0;
    }
    #mygif {
      position: fixed;
      right: 0;
      bottom: 0;
      min-width: 100%;
      min-height: 100%;
    }
    {{/section}}
  
    
  
  <div class="cover">
  <img src="/img/crops.gif" id="mygif" alt="...">
    <header id="header-wrap">
      <!-- Navbar Start -->
      <nav
        class="navbar navbar-expand-lg fixed-top scrolling-navbar"
        style="background-color: #fff;"
      >
        <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="navbar-header">
            <button
              class="navbar-toggler"
              type="button"
              data-toggle="collapse"
              data-target="#main-navbar"
              aria-controls="main-navbar"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <span class="navbar-toggler-icon"></span>
              <span class="icon-menu"></span>
              <span class="icon-menu"></span>
              <span class="icon-menu"></span>
            </button>
            <img src="/img/logo.png" class="navbar-brand" style="width: 250px; height: 70px" alt=""/>
          </div>
          <div class="collapse navbar-collapse" id="main-navbar" style="margin-left: 20%;">
            <ul class="navbar-nav mr-auto w-100">
              <li class="nav-item active">
                <a class="nav-link" href="dashboard">Dashboard</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="logout">Logout</a>
              </li>
            </ul>
          </div>
        </div>

        <ul class="mobile-menu">
          <li>
            <a class="page-scroll" href="dashboard">Dashboard</a>
          </li>
          <li>
            <a class="page-scroll" href="logout">Logout</a>
          </li>
        </ul>
      </nav>
    </header>
    <div style="color: red; position:absolute">
  <h3>{{messageFailure}}</h3>
</div>

<div style="color: green; position:absolute">
  {{messageSuccess}}
</div>
    <div class="content">
        <br><br><br><br>
        <div style="text-align: right; position: sticky; padding-right: 50px">
          <span style="background-color: #fff; font-size: 20px; color:#016168; padding: 10px; opacity: 80%;">Welcome Admin!</span>
        </div>
        <br>
        <div style="text-align: center; position: sticky;">
        <h1> <span style="background-color: #fff; font-size: 40px; color:#016168; padding: 15px; opacity: 80%;">Dashboard</span> </h1>
        </div>
        <br>
        <section id="form-dom">
        <div class="container" style="margin-bottom: 30px; opacity: 90%;">
          <div class="row">
            <div class="col-12">
              <div class="col-12 col-md-12 col-lg-8 log-style" style="padding: 5px;">
                <table style="width: 100%;">
                    <tr>
                        <th>Name of user</th>
                        <th>Email id</th>
                        <th>Type</th>
                        <th>Verify</th>
                    </tr>
                    {{#if users}}
                    {{#each users}}
                    <tr>
                        <td>{{username}}</td>
                        <td>{{email}}</td>
                        <td>{{type}}</td>
                        <td><a href="verify_profile/{{_id}}" class="btn btn-common btn-nv-sty">Verify</a></td>
                    </tr>
                    {{/each}}
                    {{/if}}
            </table>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>
    
    <!-- Go to Top Link -->
    <a href="#" class="back-to-top">
        <i class="lni-chevron-up"></i>
    </a>
    
    <script>
        function openForm() {
            document.getElementById("myForm").style.display = "block";
        }
        function closeForm() {
            document.getElementById("myForm").style.display = "none";
        }
    </script>
    <script>
        $(document).ready(function() {
            $(".fancybox").fancybox({
                openEffect: "none",
                closeEffect: "none"
            });
            $(".zoom").hover(function() {
                $(this).addClass('transition');
            }, function() {
                $(this).removeClass('transition');
            });
        });
    </script>
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    </div>